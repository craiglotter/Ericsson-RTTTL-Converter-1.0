\par }{\f1\fs20\insrsid7296484 ;
\par ; Ringing Tones text transfer language (RTTTL)
\par ;
\par ; Version 1.0 / 29-July-1998
\par ;
\par ;
\par 
\par <ringing-tones-text-transfer-language> :=
\par <name> <sep> [<defaults>] <sep> <note-command>+
\par 
\par <name> := <char>+ ; maximum name length 10 characters
\par 
\par <sep> := ":"
\par 
\par <defaults> :=
\par <def-note-duration> |
\par <def-note-scale> |
\par <def-beats>
\par 
\par <def-note-duration> := "d=" <duration>
\par 
\par <def-note-scale> := "o=" <scale>
\par 
\par <def-beats> := "b=" <beats-per-minute>
\par 
\par <beats-per-minute> := 25,28,...,900 ; decimal value
\par 
\par ; If not specified, defaults are
\par ;
\par ; 4 = duration
\par ; 6 = scale
\par ; 63 = beats-per-minute
\par 
\par 
\par <note-command> :=
\par [<duration>] <note> [<scale>] [<special-duration>] <delimiter>
\par 
\par 
\par <duration> :=
\par "1" | ; Full 1/1 note
\par "2" | ; 1/2 note
\par "4" | ; 1/4 note
\par "8" | ; 1/8 note
\par "16" | ; 1/16 note
\par "32" | ; 1/32 note
\par 
\par <note> :=
\par "P" | ; pause
\par "C" |
\par "C#" |
\par "D" |
\par "D#" |
\par "E" |
\par "F" |
\par "F#" |
\par "G" |
\par "G#" |
\par "A" |
\par "A#" |
\par "H"
\par 
\par <scale> :=
\par "5" | ; Note A is 440Hz
\par "6" | ; Note A is 880Hz
\par "7" | ; Note A is 1.76 kHz
\par "8" ; Note A is 3.52 kHz
\par 
\par <special-duration> :=
\par "." ; Dotted note
\par 
\par <delimiter> := ","
\par 
\par ; End of specification
\par }{\f1\fs20\insrsid7296484 
\par }}